{
  "name": "gbif-react-components",
  "version": "1.0.0",
  "description": "React components for use in GBIFs coming websites",
  "module": "./build/esm/index.js",
  "main": "./build/cjs/index.cjs",
  "browser": "./dist/lib/gbif-react-components.js",
  "exports": {
    "import": "./build/esm/index.js",
    "default": "./build/cjs/index.cjs"
  },
  "scripts": {
    "build": "path-exists .env.production.json && shx cp .env.production.json .env.json && echo \"Using .env.production.json\";  npx webpack --mode=production",
    "build:test": "path-exists .env.development.json && shx cp .env.development.json .env.json && echo \"Build with .env.development.json \";  npx webpack --mode=development",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve-translations": "npx serve locales/dist/ -p 5002 --cors",
    "watch-translations": "npx nodemon locales/build.js -w locales/ -i locales/dist/",

    "prestart": "echo \"This app requires node 16.13.1 \nCurrent version is \" ; node -v",
    "start": "path-exists .env.development.json && shx cp .env.development.json .env.json && echo \"Running with .env.development.json\"; node locales/build.js; start-storybook -p 6006;",
    "build-storybook": "npm run build-locales; build-storybook -o ./dist",
    "build-client": "npm run build-locales; npm run build-storybook; npm run build; npx gulp; npx gzipper compress --brotli --include js,css,html,txt,json,map ./dist",
    "build-locales": "node locales/build",
    "generate": "npx plop --plopfile ./tools/plopfile.js",
    "add-component": "npm run generate component",
    "add-range-filter": "npm run generate rangeFilter",
    "add-enum-filter": "npm run generate enumFilter",
    "add-suggestKey-filter": "npm run generate suggestKeyFilter",
    "add-suggest-filter": "npm run generate suggestFilter",
    "add-text-filter": "npm run generate textFilter"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/react": "^11.1.5",
    "axios": "^0.24.0",
    "d3": "^7.1.1",
    "doi-regex": "^0.1.10",
    "dompurify": "^2.2.7",
    "downshift": "^6.1.0",
    "fast-deep-equal": "^3.1.3",
    "fast-json-stable-stringify": "^2.1.0",
    "history": "^5.0.0",
    "js-base64": "^3.6.0",
    "latlon-geohash": "^2.0.0",
    "mapbox-gl": "^2.1.1",
    "markdown-it": "^13.0.1",
    "match-sorter": "^6.3.0",
    "nanoid": "^3.1.22",
    "object-hash": "^2.1.1",
    "ol": "^6.14.1",
    "ol-mapbox-style": "^8.1.2",
    "proj4": "^2.8.0",
    "query-string": "^7.0.0",
    "queue-promise": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-fast-compare": "^3.2.0",
    "react-icons": "4.4.0",
    "react-intl": "^5.13.4",
    "react-markdown": "^5.0.3",
    "react-router-dom": "^5.2.0",
    "react-use": "^17.2.1",
    "reakit": "^1.3.6",
    "uncontrollable": "^7.2.1",
    "url-regex": "^5.0.0",
    "use-debounce": "^6.0.0",
    "use-query-params": "1.2.3",
    "wicket": "1.3.8"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@storybook/addon-a11y": "^6.4.12",
    "@storybook/addon-actions": "^6.4.12",
    "@storybook/addon-knobs": "^6.1.21",
    "@storybook/addon-links": "^6.4.12",
    "@storybook/addon-storysource": "^6.4.12",
    "@storybook/addons": "^6.4.12",
    "@storybook/react": "^6.4.12",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "flat": "^5.0.2",
    "gulp": "^4.0.2",
    "gulp-if": "^3.0.0",
    "gulp-replace": "^1.0.0",
    "gzipper": "^6.2.1",
    "md5-file": "^5.0.0",
    "nodemon": "^2.0.12",
    "plop": "^2.7.4",
    "pseudoloc": "^1.1.0",
    "regenerator-runtime": "^0.13.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "path-exists-cli": "^2.0.0",
    "shx": "^0.3.4"
  }
}
