      "graphQuery": "\nquery OccurrenceSearch($predicate: Predicate) {\n  occurrenceSearch(predicate: $predicate) {\n    facet {\n      countryCode(size: 20) {\n        key\n        count\n      }\n    }\n  }\n}\n",
